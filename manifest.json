{
	"manifest_version": 3,
	"name": "PrimeMate",
	"version": "2.0",
	"description": "Primer Generator for Ensembl",
	"icons": {
		"128": "assets/icon128.png",
		"48": "assets/icon48.png"
	},
	"action": {
		"default_title": "hello",
		"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"https://uswest.ensembl.org/*",
				"https://useast.ensembl.org/*",
				"https://www.ensembl.org/*",
				"https://asia.ensembl.org/*"
			],
			"js": ["content.js"]
		}
	]
}
